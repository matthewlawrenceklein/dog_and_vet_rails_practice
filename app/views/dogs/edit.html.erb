<% if @dog.errors.any? %>
  <div id="error_explanation">
    <h2>
      <%= pluralize(@dog.errors.count, "error") %>
      prohibited this dog from being saved:
    </h2>
 
    <ul>
    <% @dog.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>


<%= form_for @dog  do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :breed %>
  <%= f.text_field :breed %>

  
  <br />

  <%= f.submit %>
<% end %>